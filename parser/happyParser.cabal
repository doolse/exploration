name:                happyParser
version:             0.1.0.0
license:             MIT
author:              Stephen Diehl
maintainer:          stephen.m.diehl@gmail.com
build-type:          Simple
extra-source-files:  README.md
cabal-version:       >=1.10

executable happyParser
  build-depends:       
      base       >= 4.6 && <5.0
    , pretty     >= 1.1 && <1.2
    , parsec     >= 3.1 && <3.2
    , containers >= 0.5 && <0.7
    , lens       >= 4
    , haskeline  >= 0.7
    , mtl        >= 2.2 && <2.4
    , transformers
  default-language:    Haskell2010
  main-is:             Main2.hs

  Build-depends:       base, array
  build-tools:         alex, happy
  other-modules:       
    Parser,
    Lexer,
    Syntax,
    Eval
